size(640,640);
background(222);
int canvas=640;
int weight=48;
int offset=14;
int gap=93;



strokeWeight(weight);
stroke(236,222,208);
line(0,offset+weight,canvas,offset+weight);
line(0,offset+weight+gap,canvas,offset+weight+gap);
line(0,offset+weight+gap*2,canvas,offset+weight+gap*2);
line(0,offset+weight+gap*3,canvas,offset+weight+gap*3);
line(0,offset+weight+gap*4,canvas,offset+weight+gap*4);
line(0,offset+weight+gap*5,canvas,offset+weight+gap*5);
